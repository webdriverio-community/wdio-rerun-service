{
  "name": "wdio-rerun-service",
  "version": "1.7.3",
  "description": "A WebdriverIO service to track and prepare for re-running failing or flaky Jasmine/Mocha tests or Cucumber Scenarios.",
  "author": "Mike Salvia <msalvia@jwplayer.com>",
  "homepage": "https://github.com/webdriverio-community/wdio-rerun-service",
  "license": "MIT",
  "main": "./build/index",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ./babel.config.js",
    "watch": "npm-watch",
    "test": "run-s test:*",
    "test:clean": "rimraf ./results ./coverage rerun.sh",
    "test:eslint": "eslint src tests",
    "test:unit": "jest --collectCoverageFrom='[\"src/**/*.{js,jsx,ts,tsx}\"]' --coverage --collectCoverage=true --forceExit --detectOpenHandles tests/.*test.*"
  },
  "watch": {
    "build": "{src,tests}/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/wdio-rerun-service.git"
  },
  "keywords": [
    "webdriver",
    "webdriverio",
    "wdio",
    "wdio-service",
    "rerun",
    "re-run",
    "retry",
    "test",
    "tests",
    "scenario",
    "cucumber",
    "mocha",
    "jasmine",
    "flaky",
    "selenium",
    "node",
    "automation",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio-community/wdio-rerun-service/issues"
  },
  "dependencies": {
    "@wdio/logger": "7.19.0",
    "minimist": "1.2.6",
    "npm-watch": "0.11.0",
    "uuid": "8.3.2"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/node": "7.16.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-proposal-optional-catch-binding": "7.16.7",
    "@babel/plugin-syntax-export-default-from": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/register": "7.17.7",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-plugin-source-map-support": "2.1.3",
    "eslint": "8.13.0",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  }
}
