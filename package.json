{
  "name": "wdio-rerun-service",
  "version": "2.1.0",
  "description": "A WebdriverIO service to track and stage for re-running failed or flaky Jasmine/Mocha tests or Cucumber Scenarios.",
  "authors": [
    "Mike Salvia <mike.salvia@icloud.com>",
    "Eric Saari <esaari@gmail.com>"
  ],
  "homepage": "https://github.com/webdriverio-community/wdio-rerun-service",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./build/cjs/index.js",
  "types": "./build/cjs/index.d.ts",
  "module": "./build/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.js"
      },
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "engines": {
    "node": ">=22.19.0"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "run-s compile:esm compile:cjs generate:cjs:package",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "generate:cjs:package": "shx echo \"{\\\"type\\\":\\\"commonjs\\\"}\" > ./build/cjs/package.json",
    "prepare": "npm run build",
    "test": "run-s test:*",
    "test:clean": "rimraf ./results ./coverage rerun.sh rerun.bat",
    "test:eslint": "eslint src tests",
    "test:tsc": "tsc -p tsconfig.json --noEmit",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "{src,tests}/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/wdio-rerun-service.git"
  },
  "keywords": [
    "automation",
    "auto-rerun",
    "bdd",
    "ci",
    "continuous-integration",
    "cucumber",
    "cucumberjs",
    "flaky",
    "flaky-tests",
    "gherkin",
    "jasmine",
    "jasmine-tests",
    "javascript",
    "javascript-testing",
    "mocha",
    "mocha-tests",
    "nodejs",
    "parallel-testing",
    "re-run",
    "rerun",
    "rerun-failures",
    "retry",
    "retry-failures",
    "retry-runner",
    "scenario",
    "selenium",
    "test",
    "test-retry",
    "tests",
    "typescript",
    "wdio",
    "wdio-service",
    "wdio-service-plugin",
    "webdriver",
    "webdriver.io",
    "webdriverio",
    "webdriverio-plugin"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio-community/wdio-rerun-service/issues"
  },
  "dependencies": {
    "@wdio/logger": "^9.18.0",
    "@wdio/types": "^9.20.0",
    "minimist": "^1.2.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.16",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@wdio/cucumber-framework": "^9.21.0",
    "@wdio/globals": "^9.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.2",
    "npm-run-all": "4.1.5",
    "npm-watch": "0.11.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "release-it": "^15.5.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "collectCoverageFrom": [
      "src/index.ts"
    ]
  }
}
